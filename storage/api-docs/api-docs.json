{
    "openapi": "3.0.0",
    "info": {
        "title": "Personnal Nutrition",
        "description": "",
        "contact": {
            "email": "support@example.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Login an user account",
                "description": "Register a new user by providing the required information",
                "operationId": "a3b306d14572d1f4bd6c064b3233e7b8",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "johndoe@example.com"
                                    },
                                    "mot_de_passe": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User registered successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "User registered successfully"
                                        },
                                        "user_id": {
                                            "type": "integer",
                                            "example": 1
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Create an user account",
                "description": "Register a new user by providing the required information",
                "operationId": "8a56853624e025573120a09a4c75d468",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "nom": {
                                        "type": "string",
                                        "example": "HUGOSS"
                                    },
                                    "prenom": {
                                        "type": "string",
                                        "example": "Henry"
                                    },
                                    "age": {
                                        "type": "integer",
                                        "example": "12"
                                    },
                                    "sexe": {
                                        "type": "string",
                                        "example": "Homme"
                                    },
                                    "poids": {
                                        "type": "float",
                                        "example": "70"
                                    },
                                    "taille": {
                                        "type": "float",
                                        "example": "171"
                                    },
                                    "tour_de_taille": {
                                        "type": "float",
                                        "example": "34"
                                    },
                                    "tour_de_hanche": {
                                        "type": "float",
                                        "example": "25"
                                    },
                                    "tour_du_cou": {
                                        "type": "float",
                                        "example": "20"
                                    },
                                    "niveau_d_activite_physique": {
                                        "type": "string",
                                        "example": "Haut"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "johndoe@example.com"
                                    },
                                    "mot_de_passe": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    },
                                    "mot_de_passe_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User registered successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "User registered successfully"
                                        },
                                        "user_id": {
                                            "type": "integer",
                                            "example": 1
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}